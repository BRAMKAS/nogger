{
  "name": "nogger",
  "preferGlobal": true,
  "version": "3.0.11",
  "description": "node logger",
  "main": "index.js",
  "bin": {
    "nogger": "bin/nogger.js"
  },
  "scripts": {
    "test": "mocha",
    "start": "cross-env NODE_ENV=production node server",
    "build": "cross-env NODE_ENV=production webpack -p",
    "devFront": "webpack-dev-server --content-base public --open",
    "dev": "concurrently --kill-others --prefix \"[{name}]\" --names \"server,webpack\" \"node server -p 8080\" \"webpack --watch\"",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paul-em/nogger.git"
  },
  "keywords": [
    "logging",
    "grep",
    "logfile",
    "monitor",
    "nogger",
    "node logger",
    "logfile reader",
    "dashboard",
    "tail",
    "cli",
    "tools",
    "sysadmin"
  ],
  "author": "Paul Em",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/paul-em/nogger/issues"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "homepage": "https://github.com/paul-em/nogger#readme",
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.1",
    "chalk": "^1.1.3",
    "concurrently": "^3.4.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^2.0.3",
    "eslint-plugin-import": "^2.2.0",
    "expect.js": "^0.3.1",
    "express": "^4.15.2",
    "express-session": "^1.15.2",
    "file-loader": "^0.11.1",
    "material-design-lite": "^1.3.0",
    "minimist": "^1.2.0",
    "mocha": "^3.4.1",
    "morgan": "^1.8.1",
    "nedb": "^1.8.0",
    "nedb-session-store": "^1.1.1",
    "node-sass": "^4.5.2",
    "request": "^2.81.0",
