    "reverse-line-reader": "^0.2.6",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "vue": "^2.3.3",
    "vue-loader": "^12.0.4",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1"
    ]
  }
